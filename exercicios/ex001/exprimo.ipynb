    "num = int(input(\"Digite um número: \"))\n",
    "primos = []\n",
    "\n",
    "for a in range(1, num + 1):\n",
    "    if num % a == 0:\n",
    "        primos.append(a)\n",
    "    else:\n",
    "        pass\n",
    "\n",
    "if len(primos) == 2:\n",
    "    print(f\"O número {num} é primo \")\n",
